{
    "name": "alltheThings",
    "description": "A real download all button for Thingiverse",
    "version": "0.0.1",
    "manifest_version": 3,
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://www.thingiverse.com/thing*"
            ],
            "js": [
                "dist/main.js"
            ],
            "run_at": "document_end"
        }
    ],
    "icons": {
        "16": "/images/icon_16.png",
        "32": "/images/icon_32.png",
        "48": "/images/icon_48.png",
        "128": "/images/icon_128.png"
    }
}